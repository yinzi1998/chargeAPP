<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChargeDetailItemActivity">

    <ImageView
        android:id="@+id/imageView_chargeitem_logo"
        android:layout_width="66dp"
        android:layout_height="55dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/logo" />

    <ImageView
        android:id="@+id/imageView_chargeitem_haohao"
        android:layout_width="232dp"
        android:layout_height="173dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_chargeitem_charge"
        app:srcCompat="@mipmap/haohaoshenghuojizhang" />

    <ImageView
        android:id="@+id/imageView_chargeitem_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_chargeitem_haohao"
        app:srcCompat="@mipmap/detail_date" />

    <TextView
        android:id="@+id/textView_chargeitem_time"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        android:textColor="#212121"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_chargeitem_time"
        app:layout_constraintTop_toBottomOf="@+id/imageView_chargeitem_haohao" />

    <ImageView
        android:id="@+id/imageView_chargeitem_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_chargeitem_time"
        app:srcCompat="@mipmap/detail_type" />

    <TextView
        android:id="@+id/textView_chargeitem_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="#212121"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_chargeitem_type"
        app:layout_constraintTop_toBottomOf="@+id/textView_chargeitem_time" />

    <ImageView
        android:id="@+id/imageView_chargeitem_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_chargeitem_type"
        app:srcCompat="@mipmap/detail_detail" />

    <TextView
        android:id="@+id/textView_chargeitem_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="#212121"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_chargeitem_detail"
        app:layout_constraintTop_toBottomOf="@+id/textView_chargeitem_type" />

    <ImageView
        android:id="@+id/imageView_chargeitem_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_chargeitem_detail"
        app:srcCompat="@mipmap/detail_money" />

    <TextView
        android:id="@+id/textView_chargeitem_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:textColor="#212121"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_chargeitem_money"
        app:layout_constraintTop_toBottomOf="@+id/textView_chargeitem_detail" />

    <TextView
        android:id="@+id/textView_chargeitem_charge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="20dp"
        android:text="账单细节"
        android:textColor="#212121"
        android:textSize="26sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_chargeitem_logo"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>